{
  "name": "TabSwitcher",
  "version": "0.1",
  "description": "Switch to another tab easily",
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["libs/zepto.js","build/hook.js"],
    "css": ["style/main.css"]
  }
  ],
  "background_page" : "background.html",
  "permissions": ["tabs"]
}

